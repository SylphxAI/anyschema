{
	"name": "@sylphx/anyschema",
	"version": "0.1.4",
	"packageManager": "bun@1.3.3",
	"description": "Universal schema utilities for TypeScript. Zero dependencies. Works with Zod, Valibot, ArkType, Yup, Joi, io-ts, and more.",
	"type": "module",
	"sideEffects": false,
	"module": "./dist/src/index.js",
	"types": "./dist/src/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/src/index.d.ts",
			"default": "./dist/src/index.js"
		},
		"./adapter": {
			"types": "./dist/src/adapter/index.d.ts",
			"default": "./dist/src/adapter/index.js"
		},
		"./adapter/*": {
			"types": "./dist/src/adapter/*.d.ts",
			"default": "./dist/src/adapter/*.js"
		}
	},
	"files": [
		"dist"
	],
	"publishConfig": {
		"access": "public"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/SylphxAI/anyschema.git"
	},
	"scripts": {
		"build": "bunup",
		"dev": "bunup --watch",
		"test": "bun test",
		"typecheck": "tsc --noEmit",
		"prepack": "bun run build",
		"prepublishOnly": "bunx @sylphx/doctor prepublish",
		"lint": "biome check .",
		"format": "biome format --write ."
	},
	"keywords": [
		"schema",
		"anyschema",
		"universal",
		"standard-schema",
		"zod",
		"valibot",
		"arktype",
		"yup",
		"joi",
		"io-ts",
		"superstruct",
		"typebox",
		"effect-schema",
		"runtypes",
		"json-schema",
		"validation",
		"type-inference",
		"typescript",
		"zero-dependency"
	],
	"author": "",
	"license": "MIT",
	"devDependencies": {
		"@biomejs/biome": "^2.3.8",
		"@sylphx/biome-config": "^0.4.0",
		"@sylphx/bump": "^1.4.11",
		"@sylphx/doctor": "^1.25.0",
		"@sylphx/tsconfig": "^0.3.0",
		"@types/node": "^22.15.32",
		"@valibot/to-json-schema": "^1.0.0",
		"arktype": "^2.1.20",
		"bunup": "^0.16.10",
		"lefthook": "^2.0.4",
		"typescript": "^5.8.3",
		"valibot": "^1.1.0",
		"zod": "^3.25.67",
		"zod-to-json-schema": "^3.24.5"
	}
}
