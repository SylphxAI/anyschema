{
  "name": "anyschema",
  "version": "0.2.0",
  "description": "Universal schema utilities - type inference, validation, JSON Schema conversion. Zero dependencies, works with any version of Zod, Valibot, ArkType.",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "schema",
    "anyschema",
    "universal",
    "zod",
    "valibot",
    "arktype",
    "json-schema",
    "validation",
    "type-inference",
    "typescript",
    "zero-dependency"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^22.15.32",
    "@valibot/to-json-schema": "^1.0.0",
    "arktype": "^2.1.20",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "valibot": "^1.1.0",
    "vitest": "^3.2.4",
    "zod": "^3.25.67",
    "zod-to-json-schema": "^3.24.5"
  }
}
